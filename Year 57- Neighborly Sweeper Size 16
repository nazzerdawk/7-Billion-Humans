-- 7 Billion Humans (2234) --
-- 57: Neighborly Sweeper --

comment 0
if w == wall:
	mem3 = foreachdir nw,w,sw,n,s,ne,e,se:
		mem3 = calc 0 x 1
	endfor
	mem4 = set 1
endif
a:
if mem4 == 1:
	step e
else:
	step w
endif
mem1 = foreachdir nw,w,sw,n,s,ne,e,se:
	if mem1 == datacube:
		mem2 = calc mem2 + 1
	endif
endfor
pickup c
write mem2
drop
mem2 = set 0
jump a


